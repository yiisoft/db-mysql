{
    "name": "yiisoft/db-mysql",
    "type": "library",
    "description": "Yii DataBase MySQL Extension",
    "keywords": [
        "yii",
        "mysql"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/db-mysql",
        "issues": "https://github.com/yiisoft/db-mysql/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "chat": "https://t.me/yii3en",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/json": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.14",
        "vimeo/psalm": "^4.18",
        "yiisoft/aliases": "^1.1|^2.0",
        "yiisoft/log-target-file": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Db\\Mysql\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Db\\Mysql\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "infection/extension-installer": true
        }
    }
}
